}

<div class="box box-primary">
    <div class="box box-body">
        <form action="/action_page.php">
            <div class="form-group">
                <label>Tên Người Giao Hàng:</label>
                <input type="text" class="form-control" autofocus>
            </div>

            <div class="form-group">
                <label>Số Điện Thoại:</label>
